{
  "expo": {
    "name": "Cocona",
    "slug": "cocona",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "cocona",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.cocona.budgetapp"
    },
    "android": {
      "package": "com.cocona.budgetapp",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-sqlite"
    ],
    "extra": {
      "eas": {
        "projectId": "3e5247bd-7dd7-478a-8f84-f660cb8b6a5e"
      }
    }
  }
}
